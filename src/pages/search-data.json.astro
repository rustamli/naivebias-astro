---
import allPosts from '../_data/all'

const searchData = {
    posts: allPosts.posts.map((post) => {
        return {
            id: post.id,
            title: post.title,
            description: post.description,
            space: 'posts',
            spaceTitle: post.postedOn
        }
    }),
    spaces: []
}
---
<pre>
{JSON.stringify(searchData, null, 2)}
</pre>